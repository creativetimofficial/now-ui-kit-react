{
  "name": "now-ui-kit-react",
  "version": "1.5.0",
  "private": true,
  "homepage": "https://demos.creative-tim.com/now-ui-kit-react",
  "dependencies": {
    "gh-pages": "3.0.0",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.9",
    "history": "5.0.0",
    "moment": "2.29.1",
    "node-sass": "6.0.0",
    "nouislider": "15.1.0",
    "react": "17.0.2",
    "react-bootstrap-switch": "15.5.3",
    "react-datetime": "3.0.4",
    "react-dom": "17.0.2",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "react-syntax-highlighter": "15.4.3",
    "reactstrap": "8.9.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && gulp licenses",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile-sass": "node-sass src/assets/scss/now-ui-kit.scss src/assets/css/now-ui-kit.css",
    "minify-sass": "node-sass src/assets/scss/now-ui-kit.scss src/assets/css/now-ui-kit.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/now-ui-kit.scss src/assets/css/now-ui-kit.css --source-map true",
    "deploy": "npm run build && gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "optionalDependencies": {
    "@babel/core": "7.14.3",
    "typescript": "4.2.4"
  }
}
